# LFS tracking for temporary video files
temp-video/*.mp4 filter=lfs diff=lfs merge=lfs -text
temp-video/*.mov filter=lfs diff=lfs merge=lfs -text
temp-video/*.avi filter=lfs diff=lfs merge=lfs -text
temp-video/*.mkv filter=lfs diff=lfs merge=lfs -text
temp-video/*.webm filter=lfs diff=lfs merge=lfs -text